{
  "name": "goetia",
  "version": "0.1.0-alpha",
  "description": "Goetia is a tool for importing and exporting data from various filetypes.",
  "author": "David Robinson <dr@monetise.io> (http://www.monetise.io)",
  "homepage": "https://github.com/abrisene/goetia#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/abrisene/goetia.git"
  },
  "bugs": {
    "url": "https://github.com/abrisene/goetia/issues"
  },
  "license": "Apache-2.0",
  "keywords": [
    "xls",
    "xlsx",
    "csv",
    "xml",
    "json",
    "yaml"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "/lib",
    "/dist"
  ],
  "scripts": {
    "test": "jest",
    "coverage": "jest --coverage && cat ./coverage/lcov.info | coveralls",
    "lint": "gts lint",
    "docs": "npx typedoc",
    "clean": "gts clean",
    "compile": "tsc",
    "fix": "gts fix",
    "preversion": "npm test",
    "postversion": "npm run docs && npm run coverage",
    "prepare": "npm run compile",
    "pretest": "npm run compile",
    "posttest": "npm run lint"
  },
  "dependencies": {
    "dotenv": "^16.0.0",
    "xlsx": "^0.18.2",
    "xml-js": "^1.6.11"
  },
  "devDependencies": {
    "@types/jest": "26.0.22",
    "@types/node": "14.14.37",
    "coveralls": "3.1.0",
    "gts": "3.1.0",
    "jest": "26.6.3",
    "ts-jest": "26.5.4",
    "typescript": "4.0.3"
  }
}
